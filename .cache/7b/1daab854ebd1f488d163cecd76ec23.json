ame":"Projects","generated":{"line":123,"column":15},"source":"Projects.js","original":{"line":50,"column":15}}],"sources":{"Projects.js":"import React, { Component } from 'react';\nimport PROJECTS from './data/projects';\n\n\nclass Project extends Component {\n  render() {\n    const { title, image, description, link, id, technology } = this.props.project;\n    return(\n\n        <div className={ id % 2 == 0 ? \"portfolio--item even\" : \"portfolio--item odd\" }>\n\n          <div className=\"img--portfolio\">\n              <img src={image} style={{ opacity: 0.7, width: '100%', height: '100%' }} />\n          </div>\n\n          <div className=\"content--portfolio\">\n              <h3 className=\"title--portfolio\">{title}</h3>\n              <p>{description}<br/>\n                  <strong>Technology:  </strong>{technology}\n              </p>\n              <form action={link} target=\"_blank\">\n                  <input className=\"preview--btn\" type=\"submit\" value=\"Preview 🎏\" />\n              </form>\n          </div>\n\n        </div>\n\n    )\n  }\n}\nclass Projects extends Component {\n  render() {\n    return (\n      <div className=\"project\" id=\"project\">\n        <h2 className=\"title title--project\">Featured Projects</h2>\n        <div className=\"portfolio\">\n          {\n            PROJECTS.map(PROJECT => {\n              return(\n                <Project key={PROJECT.id} project={PROJECT} />\n              )\n            })\n          }\n\n        </div>\n      </div>\n    )\n  }\n}\nexport default Projects;\n"},"lineCount":null}},"error":null,"hash":"97cb7587fa15a5bb1fcd9c04e54496d2","cacheData":{"env":{}}}